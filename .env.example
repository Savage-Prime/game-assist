# Development Environment (Default)
# Copy this file to .env and fill in your development bot credentials

# Development Discord Bot (for testing in your test server)
DEV-DISCORD_TOKEN=your_dev_bot_token_here
DEV-APPLICATION_ID=your_dev_app_id_here

# Test Guild ID (for fast command deployment during development)
GUILD_ID=your_test_guild_id_here

# Production Environment (set these as environment variables in production)
# DISCORD_TOKEN=your_prod_bot_token_here
# APPLICATION_ID=your_prod_app_id_here
# NODE_ENV=production

TRACE_LOG=OFF